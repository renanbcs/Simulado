<?xml version="1.0" encoding="utf-8"?>
<TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.renan.simulado.ResultadoActivity">

    <TableRow
        android:layout_height="wrap_content"
        android:layout_width="fill_parent"
        android:gravity="left">

        <TextView
            android:id="@+id/nomeTextView1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            tools:layout_editor_absoluteX="16dp"
            tools:layout_editor_absoluteY="16dp"
            android:layout_marginTop="16dp"
            android:layout_marginBottom="20dp"
            android:textColor="#000000"
            android:textSize="20dp"/>

    </TableRow>

    <TableRow
        android:layout_height="wrap_content"
        android:layout_width="fill_parent"
        android:gravity="left">
        <TextView
            android:id="@+id/livrosTextView"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Eis uma lista de livro do seu interesse"
            tools:layout_editor_absoluteX="16dp"
            tools:layout_editor_absoluteY="74dp"
            android:layout_marginTop="16dp"
            android:layout_marginBottom="20dp"
            android:textColor="#000000"
            android:textSize="20dp"/>

    </TableRow>

    <TableRow
        android:layout_height="wrap_content"
        android:layout_width="fill_parent"
        android:gravity="left">

    <ListView
        android:id="@+id/livrosListView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:layout_marginBottom="20dp"></ListView>

    </TableRow>


</TableLayout>
